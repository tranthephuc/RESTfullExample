#!/usr/bin/env groovy

pipeline {
    agent any

    stages {
        stage('Init') {
            steps {
                echo 'Initing...'
            }
        }
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        stage('Package') {
            steps {
                echo 'Packaging...'
            }
        }
        stage('PreDeploy') {
            steps {
                echo 'PreDeploying....'
            }
        }
        stage('Deploy (Dev)') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}
